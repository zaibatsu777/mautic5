        "[web-root]/media/dashboards/email-overview.json": "assets/scaffold/media/dashboards/email-overview.json",
        "[web-root]/media/dashboards/stats-overview.json": "assets/scaffold/media/dashboards/stats-overview.json",
        "[web-root]/media/js/mautic-form-src.js": "assets/js/mautic-form-src.js",
        "[web-root]/media/css/mediaelementplayer.css": "assets/css/mediaelementplayer.css",
        "[web-root]/media/css/mediaelementplayer.min.css": "assets/css/mediaelementplayer.min.css",
        "[web-root]/media/css/modal.css": "assets/css/modal.css",
        "[web-root]/media/css/modal.min.css": "assets/css/modal.min.css",
        "[web-root]/media/files/.htaccess": "assets/scaffold/media/files/.htaccess",
        "[web-root]/media/images/.htaccess": "assets/scaffold/media/images/.htaccess",
        "[web-root]/themes/blank.png": "assets/scaffold/files/blank.png",
        "[web-root]/themes/blank-big.png": "assets/scaffold/files/blank-big.png"

      }
    }
  },
  "scripts": {
    "pre-autoload-dump": "Mautic\\ComposerBundle\\ComposerBundle::preAutoloadDump"
  }
}
